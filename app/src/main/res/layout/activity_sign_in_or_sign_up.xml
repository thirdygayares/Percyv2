<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/signinbg"
    tools:context=".SignInOrSignUp">

    <ImageView
        android:id="@+id/SIOSUPrecylogo"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="40dp"
        android:src="@drawable/logoprecy"/>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/SIOSUsigninbtn"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/SIOSUPrecylogo"
        android:layout_marginStart="50dp"
        android:background="@drawable/round_btn"
        android:fontFamily="@font/catamaranbold"
        android:text="@string/log_in"
        android:textColor="@color/white"
       />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/SIOSUsignupbtn"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/SIOSUPrecylogo"
        android:layout_marginStart="225dp"
        android:background="@drawable/roundbtn_white"
        android:fontFamily="@font/catamaranbold"
        android:text="@string/sign_up"/>



</RelativeLayout>